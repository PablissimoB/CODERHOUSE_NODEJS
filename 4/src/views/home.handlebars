<h1>Lista de productos</h1>
<hr>
{{#each products}}
    <div class="card text-bg-success mb-3" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">{{this.description}}</p>
            <p class="card-text">precio : {{this.price}} - stock: {{this.stock}}</p>
            <button onclick="deleteProduct('{{this.id}}');" " class="btn btn-danger"> delete</button>
        </div>
    </div>

{{/each}}